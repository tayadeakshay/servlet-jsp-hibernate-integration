<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  
  
  <servlet>
  <servlet-name>createStudent</servlet-name>
  <servlet-class>com.prowings.controller.CreateStudentController</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>createStudent</servlet-name>
  <url-pattern>/addStudent</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <servlet-name>getStudent</servlet-name>
  <servlet-class>com.prowings.controller.GetStudentController</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>getStudent</servlet-name>
  <url-pattern>/getStudent</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <servlet-name>getAllStudent</servlet-name>
  <servlet-class>com.prowings.controller.GetAllStudentController</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>getAllStudent</servlet-name>
  <url-pattern>/getAllStudent</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <servlet-name>updateStudent</servlet-name>
  <servlet-class>com.prowings.controller.UpdateStudentController</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>updateStudent</servlet-name>
  <url-pattern>/updateStudent</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <servlet-name>deleteStudent</servlet-name>
  <servlet-class>com.prowings.controller.DeleteStudentController</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>deleteStudent</servlet-name>
  <url-pattern>/deleteStudent</url-pattern>
  </servlet-mapping>
  
  <welcome-file-list>
        <welcome-file>createStudent.jsp</welcome-file>
    </welcome-file-list>
</web-app>
